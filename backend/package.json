{
  "name": "covitrack-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "sideEffects": false,
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^8.0.29",
    "@types/numeral": "^0.0.28",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.2.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "numeral": "^2.0.6",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "dotenv concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "prettify": "prettier --write . ",
    "loadFullData": "dotenv node -e \"require('./dist/owidLoader.js').fetchAndLoadFullData()\"",
    "loadLatestData": "dotenv node -e \"require('./dist/owidLoader.js').fetchAndLoadLatestData()\"",
    "loadGeographyData": "dotenv node -e \"require('./dist/geographyLoader.js').fetchAndLoadGeographyData()\""
  },
  "nodemonConfig": {
    "ext": "ts,js,json",
    "ignore": [
      ".git/*",
      "node_modules/*",
      "__tests__/*"
    ]
  }
